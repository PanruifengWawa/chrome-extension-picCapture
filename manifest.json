{
	"name": "Picture Capture",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Download pictures from Pages.",
	"icons": {"128": "img/icon_128.png"},
	"browser_action": {
		"default_title": "picCap",
		"default_icon": "img/icon_128.png",
		"default_popup": "index.html"
	},
	"content_scripts": [
   		{
      	"matches": ["http://*/*","https://*/*"],
      	"js": ["js/content.js"]
    		}
  	]
}